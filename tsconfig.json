{
	"compilerOptions": {
		"allowJs": true,
		"target": "es2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist"
	},
	"include": ["src/**/*.ts", "src/utils/config.ts"],
	"exclude": ["node_modules"]
}
